<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="100" prefWidth="75" style="-fx-border-color: #000; -fx-border-width: 2px; -fx-background-color: lightgray;" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="tegTop" alignment="CENTER" contentDisplay="CENTER" prefHeight="25" prefWidth="25" text="2">
         <font>
            <Font size="18.0" />
         </font></Label>
        <Label fx:id="tegBottom" alignment="CENTER" contentDisplay="CENTER" layoutX="50.0" layoutY="75.0" prefHeight="25" prefWidth="25" rotate="180.0" text="2">
         <font>
            <Font size="18.0" />
         </font></Label>
        <ImageView fx:id="sortTop" fitHeight="20.0" fitWidth="20.0" layoutX="50.0" layoutY="5.0" preserveRatio="true">
         <image>
            <Image url="@myndir/hjarta.png" />
         </image></ImageView>
      <ImageView fx:id="sortBottom" fitHeight="20.0" fitWidth="20.0" layoutX="5.0" layoutY="75.0" preserveRatio="true" rotate="180.0">
         <image>
            <Image url="@myndir/hjarta.png" />
         </image>
      </ImageView>
    </children>
</fx:root>
