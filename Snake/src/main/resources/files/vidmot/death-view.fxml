<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="1024.0" prefWidth="1024.0" stylesheets="@styles/death.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="files.vidmot.DeathController">
   <children>
      <Pane id="background-pane" prefHeight="1024.0" prefWidth="1024.0">
         <children>
            <Button layoutX="300.0" layoutY="913.0" mnemonicParsing="false" onAction="#sceneSwitchGame" text="Nyr leikur" />
            <Button layoutX="643.0" layoutY="913.0" mnemonicParsing="false" onAction="#sceneSwitchMenu" text="Heim" />
         </children>
      </Pane>
      <HBox layoutX="256.0" layoutY="64.0" prefHeight="96.0" prefWidth="512.0">
         <children>
            <GridPane fx:id="playerScoreGP" alignment="CENTER" prefWidth="512.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
         </children>
      </HBox>
      <VBox layoutX="256.0" layoutY="384.0" prefHeight="480.0" prefWidth="512.0" styleClass="border">
         <children>
            <GridPane prefHeight="512.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
         </children>
      </VBox>
      <Button layoutX="448.0" layoutY="224.0" mnemonicParsing="false" onAction="#submit" prefHeight="64.0" prefWidth="128.0" styleClass="score-submit-button" stylesheets="@styles/buttons.css">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <opaqueInsets>
            <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
         </opaqueInsets>
      </Button>
      <TextField fx:id="nameInput" layoutX="480.0" layoutY="192.0" onKeyTyped="#textCheck" prefWidth="64.0" />
      <Label alignment="CENTER" layoutX="448.0" layoutY="320.0" prefHeight="32.0" prefWidth="128.0" styleClass="border" text="TOP 5 PLAYERS" />
   </children>
</AnchorPane>
